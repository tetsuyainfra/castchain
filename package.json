    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "styled-components": "^5.0.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.5.1"
  },
