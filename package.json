{
  "name": "castchain",
  "version": "0.0.1",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "start": "electron build",
    "start:version": "electron build --version",
    "start:init-config": "electron build --init-config",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "build": "run-p build:*",
    "build:js": "webpack",
    "build:cp": "cpx \"src/**/*.json\" build ",
    "watch": "run-p watch:**",
    "watch:js:main": "webpack --watch --config-name main",
    "watch:js:renderer": "webpack-dev-server --watch --config-name renderer",
    "watch:js:node": "webpack --watch --config-name node",
    "watch:cp": "cpx \"src/**/*.json\" build --watch",
    "_watch:test": "jest --watch",
    "dev:build": "run-s dev:build:*",
    "dev:build:types": "tsc --emitDeclarationOnly",
    "dev:build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --source-maps inline",
    "dev:build:cp": "cpx \"src/**/*.{html,json}\" build ",
    "clean": "rimraf ./build",
    "test": "jest"
  },
  "author": "tetsuyainfra",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
